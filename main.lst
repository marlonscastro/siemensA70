.................... char invByte(char bt){    // Essa função é necessária para inverter os bits da maneira correta ex: 1100 0000 = 0000 0011 
....................    char btInvert = 0x00; 
....................    Byte bitTest = 0x80, bitset = 0x01; 
....................    for(i=0;i<8;i++){ 
....................       if((bt&bitTest) != 0) 
....................          btInvert |= bitset;  
....................       bitTest>>=1; 
....................       bitset<<=1; 
....................    } 
....................    return btInvert;    
.................... } 
